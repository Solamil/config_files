defaults
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-bundle.crt 
tls_starttls off 
# tls_nocertcheck
logfile ~/.msmtp.log


account tul
<insert from email address here>
<insert user email address here>
passwordeval "pass show Email/tul"
<insert smtp server address here>
port 465 


account seznam
<insert from email address here>
<insert user email address here>
passwordeval "pass show Email/seznam"
<insert smtp server address here>
port 465  
